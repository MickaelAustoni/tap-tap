{
  "expo": {
    "name": "TapTap",
    "description": "Touch your mobile to count.",
    "slug": "taptap",
    "privacy": "public",
    "sdkVersion": "30.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#333333"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.counter.taptap",
      "supportsTablet": true
    },
    "android": {
      "package": "com.counter.taptap",
      "permissions": [
        "READ_PHONE_STATE"
      ]
    }
  }
}
